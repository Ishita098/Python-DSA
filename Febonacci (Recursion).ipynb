   "source": [
    "def febonacci(n):\n",
    "    assert n>=0 and int(n) ==n, \"The num should be positive\"\n",
    "    if n in [0,1]:\n",
    "        return n\n",
    "    else :\n",
    "        return febonacci(n-1) + febonacci(n-2)\n",
    "print (febonacci(7))"
   ]
