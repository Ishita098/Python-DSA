   "source": [
    "def decimaltobinary(n):\n",
    "    assert int(n)==n , \"The parameter must be *ve\"\n",
    "    if n==0:\n",
    "        return 0\n",
    "    else:\n",
    "        return n%2 + 10*decimaltobinary(int(n/2))\n",
    "print(decimaltobinary(2))"
   ]
