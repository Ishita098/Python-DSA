    "def gcd(a,b):\n",
    "    assert int(a)==a and int(b)== b , \"The integer must be a positive integer\"\n",
    "    if a<0:\n",
    "        a = -1*a\n",
    "    if b<0:\n",
    "        b= -1*b\n",
    "    if b==0:\n",
    "        return a\n",
    "    else:\n",
    "        return gcd( b , a%b)\n",
    "print(gcd(6 ,1.8))\n",
    "        "
